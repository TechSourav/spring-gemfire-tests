<?xml version="1.0" encoding="utf-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
        http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd
">

  <context:annotation-config/>

  <bean id="addition" class="org.spring.data.gemfire.cache.execute.support.CalculatorSupport" factory-method="addition"/>

  <bean id="division" class="org.spring.data.gemfire.cache.execute.support.CalculatorSupport" factory-method="division"/>

  <bean id="exponential" class="org.spring.data.gemfire.cache.execute.support.CalculatorSupport" factory-method="exponential"/>

  <bean id="multiplication" class="org.spring.data.gemfire.cache.execute.support.CalculatorSupport" factory-method="multiplication"/>

  <bean id="subtraction" class="org.spring.data.gemfire.cache.execute.support.CalculatorSupport" factory-method="subtraction"/>

</beans>
