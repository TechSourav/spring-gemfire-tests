<?xml version="1.0"?>
<!DOCTYPE client-cache PUBLIC  "-//GemStone Systems, Inc.//GemFire Declarative Caching 8.0//EN"
  "http://www.gemstone.com/dtd/cache8_0.dtd">
<client-cache>
  <pool name="serverPool">
    <server host="localhost" port="12480"/>
  </pool>
  <region name="Example">
    <region-attributes refid="CACHING_PROXY" scope="distributed-ack">
      <!--partition-attributes redundant-copies="1" total-num-buckets="113"/-->
      <membership-attributes loss-action="no-access" resumption-action="none">
        <required-role name="test"/>
      </membership-attributes>
      <subscription-attributes interest-policy="cache-content"/>
    </region-attributes>
  </region>
</client-cache>
